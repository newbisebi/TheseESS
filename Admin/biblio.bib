@article{pascal2018les,
	title = {Les méthodes mixtes en systèmes d’information : enjeux épistémologiques et méthodologiques},
	volume = {Volume 23},
	issn = {1260-4984},
	shorttitle = {Les méthodes mixtes en systèmes d’information},
	url = {http://www.cairn.info/revue-systemes-d-information-et-management-2018-3-page-99.htm},
	abstract = {Les m\&\#233;thodes mixtes rencontrent un succ\&\#232;s croissant dans les sciences sociales aujourd\&\#8217;hui, succ\&\#232;s en partie li\&\#233; \&\#224; leur capacit\&\#233; \&\#224; g\&\#233;n\&\#233;rer des r\&\#233;sultats de recherche qui diff\&\#232;rent de ceux obtenus par les m\&\#233;thodologies traditionnelles. \&\#192; ce titre, les m\&\#233;thodes mixtes semblent f\&\#233;condes pour le domaine des Syst\&\#232;mes d\&\#8217;Information (SI) notamment car elles offrent un large \&\#233;ventail de choix m\&\#233;thodologiques (Mingers, 2001\&\#160;; Venkatesh et al., 2013). Cependant, nous disposons de peu de connaissance sur la fa\&\#231;on dont les auteurs s\&\#8217;approprient justement ces m\&\#233;thodes et sur les choix qu\&\#8217;ils effectuent. Dans cette perspective, cet article a pour objet de mieux comprendre l\&\#8217;utilisation des m\&\#233;thodes mixtes faite par les chercheurs dans le domaine des SI afin d\&\#8217;appr\&\#233;hender s\&\#8217;il existe des tendances quant \&\#224; l\&\#8217;usage de ce type de m\&\#233;thode et si ces m\&\#233;thodes sont rigoureusement mises en \&\#339;uvre. Pour ce faire, \&\#224; partir d\&\#8217;une revue de litt\&\#233;rature comparative sur les m\&\#233;thodes mixtes en sciences sociales et en SI, une analyse bibliographique sur sept journaux en SI a \&\#233;t\&\#233; conduite sur la p\&\#233;riode 2008-2016. Cette analyse nous permet d\&\#8217;apporter un regard critique sur l\&\#8217;utilisation des m\&\#233;thodes mixtes en SI et de compl\&\#233;ter ainsi les deux \&\#233;tudes bibliographiques r\&\#233;alis\&\#233;es par Mingers (2001) et Venkatesh et al. (2013). En ancrant sa r\&\#233;flexion dans l\&\#8217;\&\#233;pist\&\#233;mologie, cette analyse se distingue alors des pr\&\#233;c\&\#233;dentes ce qui en fait son originalit\&\#233; et sa force. Elle nous permet alors de proposer des recommandations pour les auteurs d\&\#233;sireux de mettre en \&\#339;uvre ce type de m\&\#233;thode.},
	language = {fr},
	number = {3},
	urldate = {2019-06-11},
	journal = {Systemes d'information management},
	author = {Pascal, Amandine and Aldebert, Bénédicte and Rouziès, Audrey},
	month = oct,
