@article{boutillier2009aux,
	title = {Aux origines de l'entrepreneuriat social. {Les} affaires selon {Jean}-{Baptiste} {André} {Godin} (1817-1888), {Abstract}},
	issn = {1267-4982},
	url = {http://www.cairn.info/resume.php?ID_ARTICLE=INNO_030_0115},
	abstract = {{\textless}titre{\textgreater}R\&\#233;sum\&\#233;{\textless}/titre{\textgreater}L\&\#8217;entrepreneur social est depuis le d\&\#233;but des ann\&\#233;es 1990 un acteur fondamental de l\&\#8217;\&\#233;conomie capitaliste. Mais, qu\&\#8217;est-ce qu\&\#8217;un entrepreneur social\&\#160;? La question fait d\&\#233;bat. Le consensus est inexistant. L\&\#8217;entrepreneur est-il social parce qu\&\#8217;il cr\&\#233;e une activit\&\#233; non lucrative\&\#160;? Dans ces conditions, comment peut-il survivre dans une \&\#233;conomie concurrentielle\&\#160;? L\&\#8217;entrepreneur est-il social parce qu\&\#8217;il finance, gr\&\#226;ce aux profits de son entreprise, des activit\&\#233;s sociales\&\#160;? Les questions sont multiples. Pour illustrer nos propos, nous avons pris l\&\#8217;exemple d\&\#8217;un entrepreneur fran\&\#231;ais du XIXe si\&\#232;cle\&\#160;: Jean-Baptiste Andr\&\#233; Godin (1817-1888) qui innove \&\#224; la fois sur les plans technique et social. Il invente un nouvel appareil de chauffage et fait fortune et cr\&\#233;e un \&\#171;\&\#160;familist\&\#232;re\&\#160;\&\#187; en s\&\#8217;inspirant des id\&\#233;es de Charles Fourier. Les ouvriers (qui le souhaitent) sont log\&\#233;s dans le familist\&\#232;re, mais b\&\#233;n\&\#233;ficient \&\#233;galement de soins m\&\#233;dicaux gratuits, les enfants sont scolaris\&\#233;s dans l\&\#8217;entreprise. Le calcul \&\#233;conomique de Godin est rationnel car son entreprise est plus rentable que ses concurrentes. Pendant plusieurs d\&\#233;cennies, le familist\&\#232;re fut leader de son march\&\#233;. La question fondamentale devient alors\&\#160;: l\&\#8217;entrepreneur social est-il paternaliste\&\#160;?Codes JEL\&\#160;: P12, 013, N83, Since the beginning of the 1990\&\#8217;s, the social entrepreneur has become a fundamental actor within capitalism. But, what is a social entrepreneur, one must ask? The question is debated and defies consensus. Is an entrepreneur social because he founds a nonprofit enterprise? If so, then how can he live in the free market economy? Alternately, is the entrepreneur social because he finances social ventures from profits? To illustrate this conundrum, consider 19th century French entrepreneur Jean-Baptiste Andr\&\#233; Godin (1817-1888) who was both a technological and a social innovator. He invented a new system of heating and also made a fortune creating a \&\#8220;familist\&\#232;re\&\#8221; according to the theory of Charles Fourier. The workers (who chose to do so) lived in the familist\&\#232;re, enjoying a large range of welfare activities (care, social insurance, etc). Their children were also educated by the enterprise. The economic strategy\&\#160;of Godin was rational because his enterprise was more profitable than others. Indeed, for many decades the familist\&\#232;re of Godin was the market leader. So, then, we must also consider a follow-on question: Is the social entrepreneur paternalistic?JEL codes: P12, 013, N83},
	language = {fr},
	number = {30},
	urldate = {2017-05-23},
	journal = {Innovations},
	author = {Boutillier, Sophie},
	month = aug,
	year = {2009},
	keywords = {innovation, économie sociale, entrepreneur},
	pages = {115--134},
	file = {Snapshot:C\:\\Users\\sebas\\Zotero\\storage\\69VN7CDQ\\revue-innovations-2009-2-page-115.html:text/html}
}

