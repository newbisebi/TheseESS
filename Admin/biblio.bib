	pages = {300--332},
	file = {SAGE PDF Full Text:C\:\\Users\\sebas\\Zotero\\storage\\4CV7UIB7\\Briscoe et al. - 2015 - Social Activism and Practice Diffusion How Activi.pdf:application/pdf}
}

