	pages = {42--56},
	file = {ScienceDirect Full Text PDF:C\:\\Users\\sebas\\Zotero\\storage\\5MPN73Q2\\Bocken et al. - 2014 - A literature and practice review to develop sustai.pdf:application/pdf}
}

